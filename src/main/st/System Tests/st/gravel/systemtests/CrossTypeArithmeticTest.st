!
generate
	| values |
	values := (Core.Dictionary new)
				at: #float put: 7.5;
				at: #double put: 100.5000003d;
				at: #fixed put: 2.5s;
				at: #fraction put: 2 / 3;
				at: #int put: 7;
				at: #big put: 251172765292171600923361522612285166173;
				at: #zero put: 0;
				yourself.
	values keysAndValuesDo: 
			[:an :a |
			values keysAndValuesDo: 
					[:bn :b |
					#(#* #+ #- #// #quo: #rem: #\\) do: 
							[:sel |
							self
								generateTest: an
								value: a
								op: sel
								arg: bn
								value: b]]]
!
